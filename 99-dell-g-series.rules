# Dell G Series Keyboard RGB USB devices
# Allow users in 'wheel' group to access Dell G Series keyboard devices

# Dell G15 5530 Keyboard Controller (IDs: 187c:0550 and 187c:0551)
SUBSYSTEM=="usb", ATTRS{idVendor}=="187c", ATTRS{idProduct}=="0550", MODE="0666", GROUP="wheel"
SUBSYSTEM=="usb", ATTRS{idVendor}=="187c", ATTRS{idProduct}=="0551", MODE="0666", GROUP="wheel"

# HID devices
SUBSYSTEM=="hidraw", ATTRS{idVendor}=="187c", ATTRS{idProduct}=="0550", MODE="0666", GROUP="wheel"
SUBSYSTEM=="hidraw", ATTRS{idVendor}=="187c", ATTRS{idProduct}=="0551", MODE="0666", GROUP="wheel"
